{
    "name": "sonata-project/formatter-bundle",
    "description": "Symfony SonataFormatterBundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": [
        "sonata",
        "formatter",
        "editor",
        "ckeditor"
    ],
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "https://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataFormatterBundle/contributors"
        }
    ],
    "homepage": "https://docs.sonata-project.org/projects/SonataFormatterBundle",
    "require": {
        "php": "^7.4 || ^8.0",
        "friendsofsymfony/ckeditor-bundle": "^2.0",
        "psr/container": "^1.0 || ^2.0",
        "psr/log": "^1.0 || ^2.0 || ^3.0",
        "sonata-project/block-bundle": "^4.11",
        "sonata-project/form-extensions": "^1.4",
        "symfony/config": "^4.4 || ^5.4 || ^6.0",
        "symfony/dependency-injection": "^4.4 || ^5.4 || ^6.0",
        "symfony/event-dispatcher": "^4.4 || ^5.4 || ^6.0",
        "symfony/form": "^4.4 || ^5.4 || ^6.0",
        "symfony/framework-bundle": "^4.4 || ^5.4 || ^6.0",
        "symfony/http-foundation": "^4.4 || ^5.4 || ^6.0",
        "symfony/http-kernel": "^4.4 || ^5.4 || ^6.0",
        "symfony/options-resolver": "^4.4 || ^5.4 || ^6.0",
        "symfony/property-access": "^4.4 || ^5.4 || ^6.0",
        "symfony/translation": "^4.4 || ^5.4 || ^6.0",
        "symfony/validator": "^4.4 || ^5.4 || ^6.0",
        "twig/twig": "^2.12.1 || ^3.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.7",
        "doctrine/doctrine-bundle": "^2.3.2",
        "doctrine/orm": "^2.9",
        "friendsofphp/php-cs-fixer": "^3.4",
        "knplabs/knp-menu-bundle": "^3.0",
        "matthiasnoback/symfony-config-test": "^4.2",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^1.4",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.0",
        "phpstan/phpstan-symfony": "^1.1",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.17",
        "psalm/plugin-symfony": "^3.1",
        "rector/rector": "^0.13",
        "sonata-project/admin-bundle": "^4.0",
        "sonata-project/classification-bundle": "^4.0",
        "sonata-project/doctrine-extensions": "^1.0",
        "sonata-project/doctrine-orm-admin-bundle": "^4.0",
        "sonata-project/media-bundle": "^4.2",
        "sonata-project/twig-extensions": "^1.0",
        "symfony/browser-kit": "^4.4 || ^5.4 || ^6.0",
        "symfony/phpunit-bridge": "^6.1",
        "symfony/security-bundle": "^4.4 || ^5.4 || ^6.0",
        "vimeo/psalm": "^4.19"
    },
    "conflict": {
        "sonata-project/admin-bundle": "<4.0",
        "sonata-project/classification-bundle": "<4.0",
        "sonata-project/media-bundle": "<4.2"
    },
    "autoload": {
        "psr-4": {
            "Sonata\\FormatterBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sonata\\FormatterBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    }
}
